{
  // wgs_aligned_input_bam_or_cram
  // is a mapped bam file
  "MitochondriaPipeline.wgs_aligned_input_bam_or_cram": "input_bam_here",
  "MitochondriaPipeline.wgs_aligned_input_bam_or_cram_index": "input_bam_index_here",
  "MitochondriaPipeline.sample_name": "SAMPLE_NAME",
  
  // autosomal_coverage
  // is median of autosomal gene coverage
  // used to descriminate between MT vs nuMT with WGS data
  // is generated by `gatk CollectHsMetrics`
  "MitochondriaPipeline.autosomal_coverage": 
  "autosomal_median_coverage_here",


  // ALL VARIABLES BELOW correspond to files in the chrM dir
    // mt_fasta
    // mt_shifted_fasta
    // blacklisted_sites
    // blacklisted_sites_shifted
    // shift_back_chain
    // control_region_shifted_reference_interval_list
    // non_control_region_interval_list


  // mt_fasta
  // is only the chrM interval of GRCh38
  "MitochondriaPipeline.mt_dict": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.dict",
  "MitochondriaPipeline.mt_fasta": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.fasta",
  "MitochondriaPipeline.mt_fasta_index": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.fasta.fai",
  "MitochondriaPipeline.mt_amb": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.fasta.amb",
  "MitochondriaPipeline.mt_ann": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.fasta.ann",
  "MitochondriaPipeline.mt_bwt": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.fasta.bwt",
  "MitochondriaPipeline.mt_pac": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.fasta.pac",
  "MitochondriaPipeline.mt_sa": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.fasta.sa",

  
  // mt_shifted_fasta
  // is chrM.hg38.fasta
  "MitochondriaPipeline.mt_shifted_dict": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.shifted_by_8000_bases.dict",
  "MitochondriaPipeline.mt_shifted_fasta": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.shifted_by_8000_bases.fasta",
  "MitochondriaPipeline.mt_shifted_fasta_index": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.shifted_by_8000_bases.fasta.fai",
  "MitochondriaPipeline.mt_shifted_amb": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.shifted_by_8000_bases.fasta.amb",
  "MitochondriaPipeline.mt_shifted_ann": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.shifted_by_8000_bases.fasta.ann",
  "MitochondriaPipeline.mt_shifted_bwt": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.shifted_by_8000_bases.fasta.bwt",
  "MitochondriaPipeline.mt_shifted_pac": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.shifted_by_8000_bases.fasta.pac",
  "MitochondriaPipeline.mt_shifted_sa": "gs://broad-references/hg38/v0/chrM/Homo_sapiens_assembly38.chrM.shifted_by_8000_bases.fasta.sa",
  
  // blacklisted_sites 
  // are sites from chrM dir
  "MitochondriaPipeline.blacklisted_sites": "gs://broad-references/hg38/v0/chrM/blacklist_sites.hg38.chrM.bed",
  "MitochondriaPipeline.blacklisted_sites_index": "gs://broad-references/hg38/v0/chrM/blacklist_sites.hg38.chrM.bed.idx",

  // blacklisted_sites_shifted
  // shifted blacklisted mito sites
  "MitochondriaPipeline.blacklisted_sites_shifted": "gs://broad-references/hg38/v0/chrM/blacklist_sites.hg38.chrM.shifted_by_8000_bases.fixed.bed",
  "MitochondriaPipeline.blacklisted_sites_shifted_index": "gs://broad-references/hg38/v0/chrM/blacklist_sites.hg38.chrM.shifted_by_8000_bases.fixed.bed.idx",
  
  // shift_back_chain
    // chain files are required for liftover
    // for how to read chain files, see 
    // https://genome.ucsc.edu/goldenPath/help/chain.html
    // line 1: chain score tName tSize tStrand(+) tStart tEnd qName qSize qStrand(+) qStart qEnd id 
      // t = target sequence (reference)
      // q = query sequence ()
      // start and end positions are zero-based half open intervals (meaning start with # before actual, end on actual)
    // line(s) 2+ "alignment data lines" (where to put gaps): size dt dq
      // in this case there are no gaps
  // is a small text file with the following 5 lines of text
    // chain   8569    chrM    16569   +       0       8569    chrM    16569   +       8000    16569   1
    // 8569
    //
    // chain   7999    chrM    16569   +       8569    16569   chrM    16569   +       0       8000    2
    // 8000
  "MitochondriaPipeline.shift_back_chain": "gs://broad-references/hg38/v0/chrM/ShiftBack.chain",
  
  // INTERVAL LISTS
    // https://software.broadinstitute.org/gatk/documentation/article?id=11009
    // can be acquired from a bait manufacturer
    // in this case, it is made by mshand from MT sequence 
    // change the path in the interval list files to equal the path that the variable mt_fasta or mt_shifted_fasta points to
    // @VN means version of SAM/BAM formatting
    // @SQ means sequence
      // SN: sequence name
      // LN: length
      // M5: hash
      // UR: URI or path to file of the sequence
    //  <chr> <start> <stop> <strand:+/-> <target_name>
      // '.' probably refers to the interval list filename


  // control_region_shifted_reference_interval_list
  // 1,119 bp interval
  // is a small text file with the following 3 lines of text
    // @HD     VN:1.5
    // @SQ     SN:chrM LN:16569        M5:8ca1f1339b6185c6f63cf3752285cc37     UR:file:/humgen/gsa-hpprojects/dev/mshand/SpecOps/Mitochondria/MitochondriaOnlyFastas/Homo_sapiens_assembly38.mt_only.shifted_by_8000_bases.fasta
    // chrM    8025    9144    +       .
  "MitochondriaPipeline.control_region_shifted_reference_interval_list": "gs://broad-references/hg38/v0/chrM/control_region_shifted.chrM.interval_list",

  // non_control_region_interval_list
  // 15,448 bp interval
  // is a small text file with the following 3 lines of text
    // @HD     VN:1.5
    // @SQ     SN:chrM LN:16569        M5:c68f52674c9fb33aef52dcf399755519     UR:file:UR:file:/humgen/gsa-hpprojects/dev/mshand/SpecOps/Mitochondria/MitochondriaOnlyFastas/Homo_sapiens_assembly38.mt_only.fasta
    // chrM    576     16024   +       .
  "MitochondriaPipeline.non_control_region_interval_list": "gs://broad-references/hg38/v0/chrM/non_control_region.chrM.interval_list"


}
